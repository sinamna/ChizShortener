# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Link{
    longLink: String!
    shortLink: String!
}

type Query {
  links:[Link!]!
  getLongLink(shortLink:String!):Link
  getShortLink(LongLink:String!):Link
}
input newLink {
  longLink: String!
}

type Mutation {
    createLink(input:newLink!): Link!
}